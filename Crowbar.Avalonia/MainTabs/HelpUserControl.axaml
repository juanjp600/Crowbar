<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Crowbar.Avalonia.MainTabs.HelpUserControl">
    <UserControl.Resources>
        <Bitmap x:Key="LearnToUseCrowbar">
            <x:Arguments>
                <x:String>./Resources/CrowbarGuideBanner.png</x:String>
            </x:Arguments>
        </Bitmap>
    </UserControl.Resources>
    <Button x:Name="LearnToUseCrowbarButton" Width="NaN" Height="NaN" VerticalAlignment="Top">
        <Image Source="{DynamicResource LearnToUseCrowbar}" Width="524" Height="141" />
    </Button>
</UserControl>
